<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <link rel="stylesheet" href="/css/styles.css">
  <title>Product List</title>
</head>
<body>
  <h1>Product List</h1>
  <ul>
    <% products.forEach(product => { %>
      <li><%= product.name %> - <%= product.category.name %> - <%= product.subcategory.name %> - <%= product.brand.name %> - $<%= product.price %></li>
    <% }) %>
  </ul>
  <a href="/">Back to Home</a>
</body>
</html>
