<!-- views/user/index.ejs -->
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Product List</title>
</head>
<body>
    <%- include('../partials/header') %>
    <h1>Product List</h1>
    <ul>
        <% products.forEach(product => { %>
            <li><%= product.name %> - <%= product.price %> - <%= product.category.name %> - <%= product.subcategory.name %> - <%= product.brand.name %></li>
        <% }) %>
    </ul>
    <%- include('../partials/footer') %>
</body>
</html>
